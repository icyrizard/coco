    <node name="VarDecList">
      <sons>
        <son name="Head">
          <targets>
            <target mandatory="yes">
              <node name="VarDec" />
              <phases>
                <all />
              </phases>
            </target>
          </targets>
        </son>
        <son name="Tail">
          <targets>
            <target mandatory="no">
              <node name="VarDecList" />
              <phases>
                <all />
              </phases>
            </target>
          </targets>
        </son>
      </sons>
      <attributes />
    </node>

    <node name="VarDec">
      <sons>
        <son name="Value">
          <targets>
            <target mandatory="no">
              <set name="Expr" />
              <phases>
                <all />
              </phases>
            </target>
          </targets>
        </son>
        <son name="Id">
          <targets>
            <target mandatory="yes">
              <node name="VarLet" />
              <phases>
                <all />
              </phases>
            </target>
          </targets>
        </son>
      </sons>
      <attributes>
        <attribute name="Type">
          <type name="TypeDef">
            <targets>
              <target mandatory="yes">
                <any />
                <phases>
